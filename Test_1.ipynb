   ],
   "source": [
    "l=input().split(',')\n",
    "score= []\n",
    "for i in l:\n",
    "    if (i==\"+\"):\n",
    "        score.append(score[-1]+score[-2])\n",
    "    elif (i.isalpha()==False):\n",
    "        score.append(int(i))\n",
    "    elif (i==\"D\"):\n",
    "        score.append(score[-1]*2)\n",
    "    elif (i==\"C\"):\n",
    "        score.remove(score[-1])\n",
    "print(sum(score))"
   ]
